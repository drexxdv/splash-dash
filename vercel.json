{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/static/*",
      "dest": "/static/*"
    },
    {
      "src": "/views/*",
      "dest": "/views/*"
    }
  ]
}
